#!/Users/saitouakira/penaltylogger2/myvenv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pylint==0.28.0','console_scripts','pylint-gui'
__requires__ = 'pylint==0.28.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pylint==0.28.0', 'console_scripts', 'pylint-gui')()
    )
